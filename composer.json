{
    "name": "talesoft/tale-http",
    "type": "library",
    "keywords": ["http", "request", "response", "psr-7"],
    "description": "A PSR-7 and PSR-17 compatible HTTP utility library",
    "license": "MIT",
    "homepage": "http://docs.talesoft.codes/php/tale/http",
    "authors": [
        {
            "name": "Torben Koehn",
            "email": "torben@talesoft.codes"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Tale\\": "src",
            "Tale\\Test\\": "tests"
        }
    },
    "provides": {
        "psr/http-message-implementation": "~1.0",
        "psr/http-factory-implementation": "~1.0"
    },
    "require-dev": {
        "talesoft/tale-dev-tool": "dev-master",
        "talesoft/tale-stream": "^0.4.0",
        "talesoft/tale-uri": "^0.1.0"
    }
}
